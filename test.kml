<?xml version='1.0' encoding='UTF-8'?>
<kml xmlns='http://www.opengis.net/kml/2.2'>
	<Document>
		<name>FoCoBeerAlert Map</name>
		<Style id='icon'>
			<IconStyle>
				<Icon>
				<href>http://icons.iconarchive.com/icons/flat-icons.com/flat/512/Beer-icon.png</href>
				</Icon>
				<hotSpot x='16' y='31' xunits='pixels' yunits='insetPixels'>
				</hotSpot>
			</IconStyle>
		</Style>
		<Style id='location-balloon-style'>
			<BalloonStyle>
				<text>
					Beer Quality Index (Out of 5): $[Rating]<br/>
					$[geDirections]
				</text>
			</BalloonStyle>
		</Style>
		<Folder>
		<name>Location Map</name>
			<Placemark>
				<name><![CDATA[Pour Brothers]]></name>
				<styleUrl>#icon</styleUrl>
					<ExtendedData>
						<Data name = 'Rating'>
							<value>4.5</value>
						</Data>
					</ExtendedData>
					<Point>
						<coordinates>-105.0769351,40.58832810.0</coordinates>
					</Point>
			</Placemark>
		</Folder>
	</Document>
</kml>
